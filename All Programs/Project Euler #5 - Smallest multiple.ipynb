    "from math import gcd\n",
    "def func():\n",
    "    T=int(input())\n",
    "    list1=[\"\"]*T\n",
    "    for i in range(0,T):\n",
    "        N=int(input())\n",
    "        lcm=1\n",
    "        for j in range(1,N+1):\n",
    "            lcm=int(lcm*j/gcd(lcm,j))\n",
    "        list1[i]=lcm\n",
    "    print(*list1,sep='\\n')\n",
    "func()"
   ]
  }
